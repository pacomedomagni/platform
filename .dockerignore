# Version Control
.git
.gitignore

# Dependencies (will be installed in container)
node_modules
.pnp
.pnp.js

# Build outputs (will be created in container)
dist
build
.next
**/.next
out

# Testing
coverage
.nyc_output
**/*.spec.ts
**/*.test.ts
**/__tests__
cypress
e2e
apps/api-e2e
apps/web-e2e

# Cache
.cache
.nx
.turbo
*.tsbuildinfo

# IDE
.idea
.vscode
*.swp
*.swo

# OS
.DS_Store
Thumbs.db

# Environment files (use Docker secrets instead)
.env
.env.local
.env.*.local

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Documentation
docs
*.md
!README.md

# Development config files
*.map
.editorconfig
.eslintcache
.prettierrc
prettier.config.js
